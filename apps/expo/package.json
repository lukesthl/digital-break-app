  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@babel/core": "^7.23.7",
    "@types/react": "~18.2.48",
    "jest": "^29.7.0",
    "jest-expo": "~50.0.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.3",
    "source-map-explorer": "^2.5.3"
  },
  "overrides": {
