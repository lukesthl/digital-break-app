{
  "name": "@acme/expo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "export": "expo export --platform web",
    "dev:client": "npx expo start --clear --dev-client",
    "lint": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "generate-themes": "tamagui generate-themes ./theme/theme-builder.ts ./theme/theme-output.ts",
    "build:development": "eas build --profile development --platform ios --local",
    "build:simulator": "eas build --profile simulator --platform ios --local",
    "build:production": "eas build --profile production --platform ios --local",
    "prebuild": "expo prebuild --clean -p ios",
    "analyze:ios": "source-map-explorer 'dist/_expo/static/js/ios/*.js' 'dist/_expo/static/js/ios/*.js.map'"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@acme/expo-exit-app": "workspace:*",
    "@acme/target-plugin": "workspace:*",
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/native": "^6.1.9",
    "@shopify/react-native-skia": "0.1.234",
    "@tamagui-extras/core": "^1.10.0",
    "@tamagui-extras/form": "^1.15.0",
    "@tamagui/animations-moti": "^1.88.5",
    "@tamagui/animations-react-native": "^1.88.5",
    "@tamagui/babel-plugin": "^1.88.5",
    "@tamagui/checkbox": "^1.88.5",
    "@tamagui/cli": "^1.88.5",
    "@tamagui/config": "^1.88.5",
    "@tamagui/font-inter": "^1.88.5",
    "@tamagui/lucide-icons": "^1.88.5",
    "@tamagui/metro-plugin": "^1.88.5",
    "@tamagui/react-native-media-driver": "^1.88.5",
    "@tamagui/shorthands": "^1.88.5",
    "@tamagui/theme-base": "^1.88.5",
    "@tamagui/themes": "^1.88.5",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "dayjs": "^1.11.10",
    "expo": "50.0.2",
    "expo-blur": "~12.9.1",
    "expo-build-properties": "~0.11.0",
    "expo-crypto": "~12.8.0",
    "expo-dev-client": "~3.3.6",
    "expo-dev-launcher": "^3.6.3",
    "expo-document-picker": "~11.10.1",
    "expo-dynamic-app-icon": "^1.2.0",
    "expo-font": "~11.10.2",
    "expo-haptics": "~12.8.1",
    "expo-linear-gradient": "~12.7.0",
    "expo-linking": "~6.2.2",
    "expo-localization": "^14.8.3",
    "expo-router": "3.4.4",
    "expo-sharing": "~11.10.0",
    "expo-splash-screen": "~0.26.3",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-web-browser": "~12.8.1",
    "i18next": "^23.7.18",
    "lottie-react-native": "6.5.1",
    "metro": "^0.80.4",
    "metro-core": "^0.80.4",
    "mobx": "^6.12.0",
    "mobx-react": "^9.1.0",
    "react": "18.2.0",
